@charset "gb2312";
body{background:url(../images/bannerBg.jpg) repeat-x;font:normal 14px/30px "microsoft yahei";color:#444;}
#wrap{overflow:hidden;zoom:1;background:url(../images/banner.jpg) top center no-repeat;}
.banner{width:1000px;height:360px;text-align:center;margin:0 auto;}
.banner p{color:#890c00;padding-top:230px;}
.banner p a.btn{display:inline-block;background:url(../images/btn.png) no-repeat;height:72px;width:274px;color:#fff;font:normal 30px/68px "microsoft yahei";margin-bottom:10px;}
.banner p a:hover.btn{display:inline-block;background:url(../images/btnHover.png) no-repeat;height:72px;width:274px;color:#fff;font:normal 30px/68px "microsoft yahei"; text-decoration:none;margin-bottom:10px;}

.nav{width:1000px;margin:0 auto;height:54px;line-height:54px;}
.nav ul li{float:left;margin:0 1px;}
.nav ul li a{color:#fff;padding:15px 35px 15px;font:normal 18px/54px "microsoft yahei";padding:15px 35px 12px\0;padding:15px 35px 12px\9\0;}
.nav ul li a:hover,.nav ul li a.current{background:#EB1617;text-decoration:none;}

.content{background:url(../images/crossword.png) repeat;}
.mainBody{width:1000px;margin:0 auto;overflow:hidden;padding-top:30px;}
.mainBody h3{background:url(../images/titleBg.gif) no-repeat;height:40px;padding:11px 0 0 22px;color:#fff;font:normal 20px/20px "microsoft yahei";}
.schedule{padding:20px 0;text-align:center;margin-bottom:20px;}


.news{overflow:hidden;margin:15px 0;}
.intro{float:left;width:285px;background:url(../images/introBg.png) center right no-repeat;padding:0 25px 0 0;}
.intro p{text-indent:30px;}
.intro p a.red{color:#A20607;}

.picNews{float:right;width:678px;margin-left:12px;}
.picNews img{float:left;width:351px;height:266px;}
.newsList{float:right;width:314px;}
.newsList ul li{height:32px;line-height:32px;border-bottom:1px dotted #dbdbdb;}
.newsList ul li a{display:inline-block;width:95%;background:url(../images/icon.gif) left center no-repeat;padding-left:10px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.content_l{float:left;width:480px;margin-top:10px;}
.content_r{float:right;width:480px;margin-top:10px;}

h4{font:normal 18px/40px "microsoft yahei";color:#ba0001;background:#fff;border:1px solid #ededed;border-bottom:2px solid #BA0001;padding-left:10px;}
h4 span{float:right;margin-right:10px;}
h4 span a{font:normal 14px/40px "microsoft yahei";}
.content_l p,.content_r p{padding:10px 5px 20px;min-height:160px;*height:230px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}

.honour p{font:normal 18px/40px "microsoft yahei";text-indent:30px;}
.honour ul{margin:15px 0;overflow:hidden;}
.honour ul li{width:321px;border:1px solid #ededed;background:#fff;min-height:220px;float:left;margin-right:15px;}
.honour ul li div.first{background:url(../images/first.gif) 10px 10px no-repeat;padding:10px 10px 10px 112px;}
.honour ul li div.two{background:url(../images/two.gif) 10px 10px no-repeat;padding:10px 10px 10px 112px;}
.honour ul li div.three{background:url(../images/three.gif) 10px 10px no-repeat;padding:10px 10px 10px 112px;}
.honour ul li em{font:normal 22px/40px "microsoft yahei";color:#ba0001;}
.honour ul li.nomargin{margin-right:0;}

